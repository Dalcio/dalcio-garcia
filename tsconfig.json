{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "@components": [
        "components"
      ],
      "@components/*": [
        "components/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@app-state": [
        "app-state"
      ],
      "@app-state/*": [
        "app-state/*"
      ],
      "@hookd": [
        "hookd"
      ],
      "@data/*": [
        "data/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@i18n": [
        "i18n"
      ],
      "@i18n/*": [
        "i18n/*"
      ],
      "@layout": [
        "layout"
      ],
      "@layout/*": [
        "layout/*"
      ],
      "@styles": [
        "styles"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@utilities": [
        "utilities"
      ],
      "@utilities/*": [
        "utilities/*"
      ]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
